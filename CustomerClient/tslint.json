{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "interface-name": false,
        "trailing-comma": false,
        "object-literal-sort-keys": false,
        "arrow-parens": false,
        "no-shadowed-variable": false,
        "no-empty-interface": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "src/serviceWorker.ts"
        ]
    }
}